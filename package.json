{
  "name": "git-commit-message",
  "displayName": "Git Commit Message",
  "description": "Quickly edit git commit message using a fixed format",
  "repository": "https://github.com/lar-ry/git-commit-message",
  "publisher": "larry-lan",
  "icon": "assets/logo.png",
  "version": "1.0.6",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "l10n": "./l10n",
  "extensionDependencies": [
    "vscode.git"
  ],
  "contributes": {
    "commands": [
      {
        "command": "larry-lan.gitCommitMessage",
        "title": "%larry-lan.gitCommitMessage.title%",
        "icon": "$(pencil)"
      }
    ],
    "menus": {
      "scm/title": [
        {
          "command": "larry-lan.gitCommitMessage",
          "when": "scmProvider == git",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "Git Commit Message",
      "properties": {
        "gitCommitMessage.language": {
          "type": "string",
          "description": "%language%",
          "enum": [
            "zh-CN",
            "en-US"
          ],
          "enumDescriptions": [
            "简体中文",
            "English"
          ],
          "default": "zh-CN"
        },
        "gitCommitMessage.customTypes": {
          "type": "array",
          "description": "%customTypes%",
          "items": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "description": "%name%"
              },
              "description": {
                "type": "string",
                "description": "%description%"
              }
            }
          },
          "default": []
        },
        "gitCommitMessage.enableJira": {
          "type": "boolean",
          "description": "%enableJira%",
          "default": true
        },
        "gitCommitMessage.jiraUrl": {
          "type": "string",
          "markdownDescription": "%jiraUrl%",
          "default": ""
        },
        "gitCommitMessage.jiraPrefix": {
          "type": "string",
          "markdownDescription": "%jiraPrefix%",
          "default": ""
        },
        "gitCommitMessage.reporters": {
          "type": "array",
          "markdownDescription": "%reporters%",
          "items": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "description": "%reporters.name%"
              },
              "email": {
                "type": "string",
                "description": "%reporters.email%"
              },
              "picked": {
                "type": "boolean",
                "description": "%picked%"
              }
            },
            "required": [
              "name",
              "email"
            ]
          },
          "default": []
        },
        "gitCommitMessage.reviewers": {
          "type": "array",
          "markdownDescription": "%reviewers%",
          "items": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "description": "%reviewers.name%"
              },
              "email": {
                "type": "string",
                "description": "%reviewers.name%"
              },
              "picked": {
                "type": "boolean",
                "description": "%picked%"
              }
            },
            "required": [
              "name",
              "email"
            ]
          },
          "default": []
        },
        "gitCommitMessage.signerName": {
          "type": "string",
          "description": "%signerName%",
          "default": ""
        },
        "gitCommitMessage.signerEmail": {
          "type": "string",
          "description": "%signerEmail%",
          "default": ""
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.78.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}