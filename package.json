{
  "name": "git-commit-message",
  "displayName": "Git Commit Message",
  "description": "%description%",
  "repository": "https://github.com/lar-ry/git-commit-message",
  "publisher": "larry-lan",
  "icon": "assets/logo.png",
  "version": "1.3.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "git",
    "commit",
    "message",
    "format",
    "template",
    "jinja2",
    "nunjucks"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "l10n": "./l10n",
  "extensionDependencies": [
    "vscode.git"
  ],
  "contributes": {
    "commands": [
      {
        "command": "larry-lan.gitCommitMessage.edit",
        "title": "%edit%",
        "icon": "$(pencil)",
        "category": "%gitCommitMessage%"
      },
      {
        "command": "larry-lan.gitCommitMessage.clear",
        "title": "%clear%",
        "icon": "$(clear-all)",
        "category": "%gitCommitMessage%"
      }
    ],
    "menus": {
      "scm/title": [
        {
          "command": "larry-lan.gitCommitMessage.edit",
          "when": "scmProvider == git",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "Git Commit Message",
      "properties": {
        "gitCommitMessage.language": {
          "type": "string",
          "description": "%language%",
          "enum": [
            "en",
            "zh-cn"
          ],
          "enumItemLabels": [
            "English",
            "中文(简体)"
          ],
          "default": "en"
        },
        "gitCommitMessage.template": {
          "type": "string",
          "description": "%template%",
          "enum": [
            "general",
            "legacy",
            "custom"
          ],
          "enumItemLabels": [
            "%general%",
            "%legacy%",
            "%custom%"
          ],
          "default": "general"
        },
        "gitCommitMessage.template.custom": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "markdownDescription": "%template.custom%",
          "default": []
        },
        "gitCommitMessage.customTypes": {
          "type": "object",
          "markdownDescription": "%customTypes%",
          "propertyNames": {
            "type": "string"
          },
          "additionalProperties": {
            "type": "string"
          },
          "default": {}
        },
        "gitCommitMessage.jira.enable": {
          "type": "boolean",
          "description": "%jira.enable%",
          "default": true
        },
        "gitCommitMessage.jira.url": {
          "type": "string",
          "markdownDescription": "%jira.url%",
          "default": ""
        },
        "gitCommitMessage.jira.prefix": {
          "type": "string",
          "markdownDescription": "%jira.prefix%",
          "default": ""
        },
        "gitCommitMessage.reporters": {
          "type": "array",
          "markdownDescription": "%reporters%",
          "items": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "description": "%reporters.name%"
              },
              "email": {
                "type": "string",
                "description": "%reporters.email%"
              },
              "picked": {
                "type": "boolean",
                "description": "%picked%"
              }
            },
            "required": [
              "name",
              "email"
            ]
          },
          "default": []
        },
        "gitCommitMessage.reviewers": {
          "type": "array",
          "markdownDescription": "%reviewers%",
          "items": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "description": "%reviewers.name%"
              },
              "email": {
                "type": "string",
                "description": "%reviewers.email%"
              },
              "picked": {
                "type": "boolean",
                "description": "%picked%"
              }
            },
            "required": [
              "name",
              "email"
            ]
          },
          "default": []
        },
        "gitCommitMessage.signer.name": {
          "type": "string",
          "description": "%signer.name%",
          "default": ""
        },
        "gitCommitMessage.signer.email": {
          "type": "string",
          "description": "%signer.email%",
          "default": ""
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "node esbuild.config.js",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "l10n": "npx @vscode/l10n-dev export --outDir ./l10n ./src && npx @vscode/l10n-dev generate-pseudo -o ./l10n/ ./l10n/bundle.l10n.json ./package.nls.json"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/nunjucks": "^3.2.6",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vscode/l10n-dev": "^0.0.35",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.27.0",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "nunjucks": "^3.2.4"
  }
}